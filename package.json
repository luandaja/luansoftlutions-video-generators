{
	"name": "comments-template",
	"version": "1.0.0",
	"description": "My Remotion video",
	"sideEffects": [
		"*.css"
	],
	"scripts": {
		"serve:mc": "vite -c motion-canvas/vite.config.ts",
		"build:mc": "tsc --project motion-canvas/tsconfig.json && vite build -c motion-canvas/vite.config.ts",
		"render:mc": "ffmpeg -framerate 30 -pattern_type glob -i './out/motion-canvas/project/*.webp' -c:v libx264 -pix_fmt yuv420p './out/mc-project.mp4'",
		"start": "remotion preview",
		"build": "remotion render",
		"upgrade": "remotion upgrade",
		"test": "eslint src --ext ts,tsx,js,jsx && tsc"
	},
	"repository": {},
	"license": "UNLICENSED",
	"devDependencies": {
		"@remotion/eslint-config": "3.3.54",
		"@types/react": "^18.0.26",
		"@types/web": "^0.0.86",
		"autoprefixer": "10.4.4",
		"css-loader": "^6.7.1",
		"eslint": "^8.30.0",
		"postcss": "8.4.12",
		"postcss-loader": "6.2.1",
		"postcss-preset-env": "7.4.3",
		"prettier": "^2.8.1",
		"style-loader": "^3.3.1",
		"tailwindcss": "^3.1.6",
		"@motion-canvas/ui": "^3.0.1",
		"@motion-canvas/vite-plugin": "^3.0.0",
		"typescript": "^4.9.5",
		"vite": "^3.0.5"
	},
	"dependencies": {
		"@remotion/cli": "3.3.54",
		"@remotion/google-fonts": "3.3.54",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"remotion": "3.3.54",
		"@motion-canvas/core": "^3.2.0",
		"@motion-canvas/2d": "^3.2.0"
	},
	"packageManager": "npm@8.19.3"
}
